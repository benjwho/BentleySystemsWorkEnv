    "version": "0.2.0",
    "configurations": [
        {
            "name": "INWC: Python Launch Active File (Bentley)",
            "type": "debugpy",
            "request": "launch",
            "program": "${file}",
            "console": "integratedTerminal",
            "python": "C:\\ProgramData\\Bentley\\PowerPlatformPython\\python\\python.exe",
            "env": {
                "INWC_PYTHON_EXE": "C:\\ProgramData\\Bentley\\PowerPlatformPython\\python\\python.exe"
            }
        },
        {
            "name": "Python Debugger: Attach using Process Id",
            "type": "debugpy",
            "request": "attach",
            "processId": "${command:pickProcess}"
        }
    ]
}
